#include <stdio.h>

int main()
{
	int num1, num2;
	scanf("%d %d", &num1, &num2);

	// 5, 3
	if (num1 > num2)
	{
		for (int j = 1; j < 10; j++)
		{
			for (int i = num1; i >= num2; i--)
			{
				printf("%d * %d = %2d   ", i, j, i * j);
			}

			printf("\n");
		}
	}
	else // 3, 6
	{
		for (int j = 1; j < 10; j++)
		{
			for (int i = num1; i <= num2; i++)
			{
				printf("%d * %d = %2d   ", i, j, i * j);
			}

			printf("\n");
		}
	}

	return 0;
}
