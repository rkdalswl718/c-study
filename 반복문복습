#include <stdio.h>
int main(void) {
	int a,b,i,j;
	scanf("%d %d",&a,&b);
	
	if(a>b){
		for(i=1; i<=9; i++){
			for(j=a; j>=b; i--){
				printf("%d * %d =%2d   ",j,i,i*j);
			}
			printf("\n");
		}
	}
		else {
			for(i=1; i<=9; i++){
				for(j=b; j>=b; b++){
					printf("%d * %d =%2d   ",j,i,i*j);
				}
				printf("\n");
			}
		}
	
}
