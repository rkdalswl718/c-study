#include <stdio.h>

int main() {
    int nums[9];
    int max = 0;
    int idx = 0;

    for (int i = 0; i < 9; i++) {
        scanf("%d", &nums[i]);

        if (nums[i] > max) {
            max = nums[i];
            idx = i + 1;
        }
    }

    printf("%d\n", max);
    printf("%d\n", idx);

    return 0;
}
