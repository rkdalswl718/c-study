#include <stdio.h>

void inputArray(int a[], int size) {
    int i;
    for (i = 0; i < size; i++) {
        scanf("%d", &a[i]);
    }
}

void outputArray(int a[], int size) {
    int i;
    for (i = 0; i < size; i++) {
        printf("%d ", a[i]);
    }
    printf("\n");
}

int insertArray(int a[], int num, int index, int size) {
    int i;
    if (size >= 10) {
        printf("배열이 꽉 찼습니다.\n");
    } else {
        for (i = size; i > index; i--) {
            a[i] = a[i - 1];
        }
        a[index] = num;
        size++;
    }
    return size;
}

int deleteArray(int a[], int index, int size) {
    int i;
    if (size <= 0) {
        printf("배열이 비어있습니다.\n");
    } else {
        for (i = index; i < size - 1; i++) {
            a[i] = a[i + 1];
        }
        size--;
    }
    return size;
}

int main() {
    int a[10], size = 0;
    size = insertArray(a, 10, 0, size);
    size = insertArray(a, 20, 1, size);
    size = insertArray(a, 30, 2, size);
    size = insertArray(a, 50, 2, size);
    outputArray(a, size);
    size = deleteArray(a, 1, size);
    outputArray(a, size);
    return 0;
}
