#include <stdio.h>

int main() {
    int n, m;
    scanf("%d %d", &n, &m);

    int arr[101][101] = {0};
    int num = 1;

    for (int k = 2; k <= n + m; k++) {
        for (int j = 1; j <= m; j++) {
            for (int i = 1; i <= n; i++) {
                if (i + j == k) {
                    arr[i][j] = num;
                    num++;
                }
            }
        }
    }

    // 배열 출력
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            printf("%d ", arr[i][j]);
        }
        printf("\n");
    }

    return 0;
}
 
