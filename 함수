#include <stdio.h>

int add(int a, int b) {
    return a + b;
}

int subtract(int a, int b) {
    return a - b;
}

int multiply(int a, int b) {
    return a * b;
}

int divide(int a, int b) {
    if (b == 0) {
        return 0; // Divide by zero case
    }
    return a / b;
}

int main() {
    int a, b;
    char op;

    scanf("%d %c %d", &a, &op, &b);

    switch (op) {
        case '+':
            printf("%d + %d = %d", a,b,add(a, b));
            break;
        case '-':
            printf("%d - %d = %d", a,b, subtract(a, b));
            break;
        case '*':
            printf("%d * %d = %d", a,b, multiply(a, b));
            break;
        case '/':
            printf("%d / %d = %d", a,b, divide(a, b));
            break;
        default:
            printf("Invalid operation\n");
            break;
    }

    return 0;
}
